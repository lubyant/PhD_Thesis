	file = {ScienceDirect Full Text PDF:files/1370/Gippius and Myslenkov - 2020 - Black Sea wind wave climate with a focus on coastal regions.pdf:application/pdf},
}

@article{amarouche_wind-sea_2024,
	title = {Wind-{Sea} and {Swell} {Climate} in the {Black} and {Azov} {Seas}, {Based} on 42-{Year} {Spectral} {Wave} {Hindcast}},
	volume = {151},
	issn = {0141-1187},
	url = {https://www.sciencedirect.com/science/article/pii/S0141118724002761},
	doi = {10.1016/j.apor.2024.104155},
	abstract = {Long-term wave climates of the Black and Azov Seas are widely assessed based on the bulk wave parameters of the mixed sea. However, studies based on spectral wave partitions are rarely documented. Assessment of the wave climate based on the individual wave systems provides a more reliable description of the sea states' characteristics and the variability in the long-term wave climate. For instance, the wind-sea climate may reflect a real change in the local climate. However, local change in swell characteristics may not necessarily reflect a change in the local climate, and it can locally influence the mixed sea climate variability. This study assessed the spatial distributions of the long-term averages and maxima of mixed seas, wind-sea, and swell partitions at annual and seasonal scales. The contributions of wind-sea and swell partitions to the mixed-sea were computed, and their spatial and temporal variability was also assessed for overall and extreme sea state conditions. Finally, the occurrence in main wave systems' directions and occurrence of crossing, opposing, and following wind-sea and swell interactions were computed. The wind-sea contribution to the mixed-sea exceeds 70 \% in all Black Sea and {\textgreater}95 \% in the Azov Sea. The swells are more dominant on the Northeastern and Northwestern Black Sea basins. Thus, the results show spatial and temporal variability in the wind-sea climate close to the mixed sea but largely different compared to the swell climate. The inter-annual trends in the swell partitions and wind-sea are inversed at some locations.},
	urldate = {2025-03-17},
	journal = {Applied Ocean Research},
	author = {Amarouche, Khalid and AkpÄ±nar, Adem},
	month = oct,
	year = {2024},
	keywords = {Wave climate, Swell, Black Sea, Azov Sea, Spectral wave partitions, Wind-Sea},
	pages = {104155},
	file = {ScienceDirect Full Text PDF:files/1372/Amarouche and AkpÄ±nar - 2024 - Wind-Sea and Swell Climate in the Black and Azov Seas, Based on 42-Year Spectral Wave Hindcast.pdf:application/pdf;ScienceDirect Snapshot:files/1373/S0141118724002761.html:text/html},
}

@article{shamji_extreme_2020,
