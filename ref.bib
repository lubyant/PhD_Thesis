@article{dang_application_2022,
	title = {Application of deep learning models to detect coastlines and shorelines},
	volume = {320},
	issn = {03014797},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0301479722013056},
	doi = {10.1016/j.jenvman.2022.115732},
	abstract = {Identifying and monitoring coastlines and shorelines play an important role in coastal erosion assessment around the world. The application of deep learning models was used in this study to detect coastlines and shorelines in Vietnam using high-resolution satellite images and different object segmentation methods. The aims are to (1) propose indicators to identify coastlines and shorelines; (2) build deep learning (DL) models to automatically interpret coastlines and shorelines from high-resolution remote sensing images; and (3) apply DL-trained models to monitor coastal erosion in Vietnam. Eight DL models were trained based on four artificial-intelligent-network structures, including U-Net, U2-Net, U-Net3+, and DexiNed. The high-resolution images collected from Google Earth Pro software were used as input data for training all models. As a result, the U-Net using an input-image size of 512 Ã 512 provides the highest performance of 98\% with a loss function of 0.16. The interpretation results of this model were used effectively for the coastline and shoreline identification in assessing coastal erosion in Vietnam due to sea-level rise in storm events over 20 years. The outcomes proved that while the shoreline is ideal for observing seasonal tidal changes or the immediate motions of current waves, the coastline is suitable to assess coastal erosion caused by the influence of sea-level rise during storms. This paper has provided a broad scope of how the U-Net model can be used to predict the coastal changes over vietnam and the world.},
	language = {en},
	urldate = {2024-12-18},
	journal = {Journal of Environmental Management},
	author = {Dang, Kinh Bac and Dang, Van Bao and Ngo, Van Liem and Vu, Kim Chi and Nguyen, Hieu and Nguyen, Duc Anh and Nguyen, Thi Dieu Linh and Pham, Thi Phuong Nga and Giang, Tuan Linh and Nguyen, Huu Duy and Hieu Do, Trung},
	month = oct,
	year = {2022},
	pages = {115732},
	file = {PDF:files/1070/Dang et al. - 2022 - Application of deep learning models to detect coastlines and shorelines.pdf:application/pdf},
}

@incollection{marghany_chapter_2020,
	title = {Chapter 6 - {Quantum} description of sea surface},
	isbn = {978-0-12-818111-9},
	url = {https://www.sciencedirect.com/science/article/pii/B9780128181119000069},
	abstract = {A few pieces of literature have described the sea surface using quantum mechanics. The main quantum theory of wave particles are used for the sea state description. To this end, from the point view of quantum mechanics, all the information about a particle is encoded in its wave function, a complex-valued feature roughly analogous to the amplitude of a wave at every factor in space. This feature evolves in accordance with the SchrÃ¶dinger equation. For particles with mass such as seawater, this equation has solutions that comply with the form of the wave equation. The propagation of such waves leads to wave-like phenomena such as interference and diffraction. In this regard, waves are instabilities that promulgate energy through space. Transmission of the energy relies on the interactions between the particles that make up the sea state across space and time. Particles move as the waves pass through, but there is no net motion of particles. This means, once a wave has passed, the particles return to their original position. Consequently, energy, not matter, is transmitted by waves.},
	urldate = {2024-12-20},
	booktitle = {Synthetic {Aperture} {Radar} {Imaging} {Mechanism} for {Oil} {Spills}},
	publisher = {Gulf Professional Publishing},
	author = {Marghany, Maged},
	editor = {Marghany, Maged},
	month = jan,
	year = {2020},
	doi = {10.1016/B978-0-12-818111-9.00006-9},
	keywords = {Sea state, Wind waves, Wave spectra, Sea roughness, Wave orbital particles},
	pages = {93--110},
	file = {ScienceDirect Full Text PDF:files/1077/Marghany - 2020 - Chapter 6 - Quantum description of sea surface.pdf:application/pdf;ScienceDirect Snapshot:files/1078/B9780128181119000069.html:text/html},
}

@article{han_directional_2022,
	title = {Directional wave spectrum estimation with ship motion responses using adversarial networks},
	volume = {83},
	issn = {0951-8339},
	url = {https://www.sciencedirect.com/science/article/pii/S0951833922000028},
	doi = {10.1016/j.marstruc.2022.103159},
	abstract = {The external environmental conditions around a vessel are essential for efficient and safe ship operation, among which the sea state is of key importance. Considering the ship as a large wave buoy, the sea state can be estimated from motion responses without extra sensors installed. This is a challenging task since the relationships between the waves and the ship motions are hard to describe accurately. Machine learning approaches can learn these mapping without an explicit model, which is promising for sea state estimation. Current machine learning approaches represent the sea state as a set of categories or a number of wave parameters while neglecting the 2D wave spectrum. This paper proposes a sea state estimation network that estimates the 2D wave spectrum along with a discrimination network. The discrimination network can detect and correct high-order inconsistencies of the spectrum. Simulation studies are performed to show that the proposed method can provide wave spectrum estimation with high accuracy.},
	urldate = {2024-12-21},
	journal = {Marine Structures},
	author = {Han, Peihua and Li, Guoyuan and Skjong, Stian and Zhang, Houxiang},
	month = may,
	year = {2022},
	keywords = {Decision support, Generative adversarial networks, Ship intelligence, Wave spectrum estimation},
	pages = {103159},
	file = {ScienceDirect Full Text PDF:files/1081/Han et al. - 2022 - Directional wave spectrum estimation with ship motion responses using adversarial networks.pdf:application/pdf;ScienceDirect Snapshot:files/1082/S0951833922000028.html:text/html},
}

