Publisher: Pergamon-Elsevier Science Ltd
WOS:000596863000045},
	keywords = {height, bivariate distributions, Joint probability, Log-transformed kernel density estimation, Mixed copula function model, rainfall, Wave height, Wave period, Wind speed},
	pages = {107937},
	file = {Full Text:files/238/Bai et al. - 2020 - Joint probability distribution of coastal winds an.pdf:application/pdf},
}

