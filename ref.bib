	annote = {Comment: Project website at https://huangwl18.github.io/language-planner},
	file = {arXiv Fulltext PDF:C\:\\Users\\luboy\\Zotero\\storage\\QJAGLFPK\\Huang et al. - 2022 - Language Models as Zero-Shot Planners Extracting .pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\luboy\\Zotero\\storage\\ZDLV5DWD\\2201.html:text/html},
