	note = {arXiv:2303.10512 [cs]},
	keywords = {Computer Science - Machine Learning, Computer Science - Computation and Language},
	annote = {Comment: The 11th International Conference on Learning Representations (ICLR 2023)},
	file = {arXiv.org Snapshot:files/427/2303.html:text/html;Full Text PDF:files/428/Zhang et al. - 2023 - Adaptive Budget Allocation for Parameter-Efficient.pdf:application/pdf},
}

