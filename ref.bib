@article{das_adhikari_coastal_2025,
	title = {Coastal vulnerability to extreme weather events: {An} integrated analysis of erosion, sediment movement, and land subsidence based on multi-temporal optical and {SAR} satellite data},
	volume = {374},
	issn = {0301-4797},
	shorttitle = {Coastal vulnerability to extreme weather events},
	url = {https://www.sciencedirect.com/science/article/pii/S0301479725000015},
	doi = {10.1016/j.jenvman.2025.124025},
	abstract = {Coastal areas undergo continuous transformations, altering their geometry under the influence of external forces like tides, waves, and extreme events. Thus, monitoring the impact of extreme weather events on coastal regions is crucial to prevent potential cascading hazards. Here, we utilized time-series optical and SAR satellite data and tide records, coupled with sophisticated analytical techniques, to analyze erosion processes, sediment transport, and vertical land movement (VLM) at an embayed sandy beach (i.e., Haeundae Beach, South Korea). We found a significant erosion rate of â4.12Â m/year during typhoon periods, compared to non-typhoon periods (â3.61Â m/year). Between 2017 and 2023, 5.63Â ha of beach area and 14,428.9Â m3 of sediment were lost due to typhoon-induced sand movement, leading to coastline retreat and foreshore erosion. The PS-InSAR-derived localized VLM trends of fast-subsiding areas (â3.57Â mm/year), combined with a current sea-level rise (2.34Â mm/year), underscores the heightened vulnerability to future extreme weather events. The findings of this study provide insights into the erosional processes, land subsidence, and sediment transport due to extreme weather events, which can contribute to the development of erosion control technologies and engineering applications to prevent cascading hazards. The proposed integrated framework can be implemented in similar coastal environments to mitigate climate-change-induced coastal vulnerabilities.},
	urldate = {2025-06-20},
	journal = {Journal of Environmental Management},
	author = {Das Adhikari, Manik and Park, Seunghee and Yum, Sang-Guk},
	month = feb,
	year = {2025},
	keywords = {Coastal erosion, Climate change, Sediment transport, Extreme weather events, Haeundae beach, Sea-level rise, Vertical land movement},
	pages = {124025},
	file = {ScienceDirect Full Text PDF:files/1449/Das Adhikari et al. - 2025 - Coastal vulnerability to extreme weather events An integrated analysis of erosion, sediment movemen.pdf:application/pdf;ScienceDirect Snapshot:files/1450/S0301479725000015.html:text/html},
}

