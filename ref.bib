	month = jul,
	year = {2020},
	note = {Place: Heidelberg
Publisher: Springer Heidelberg
WOS:000541341200009},
	keywords = {Great Lakes, variability, wind-waves, coastal regions, ocean waves, cover, finite-volume, fvcom-swave-ug-cice, general-circulation, Ice-induced wave attenuation, pancake ice, propagation, sea-ice, Wave dynamics},
	pages = {991--1003},
