@article{davidson-arnott_wave_1980,
	title = {Wave {Climate} and {Potential} {Longshore} {Sediment} {Transport} {Patterns}, {Nottawasaga} {Bay}, {Ontario}},
	volume = {6},
	issn = {0380-1330},
	url = {https://www.sciencedirect.com/science/article/pii/S0380133080720828},
	doi = {10.1016/S0380-1330(80)72082-8},
	abstract = {A storm wave climate for Nottawasaga Bay, Ontario, is hindcast, using the SMB technique, from wind records at Cove Island for the period 1966-1970. Thirteen wave classes, defined on the basis of significant wave height, period, and direction, are identified and these are used as inputs into a computer model of wave refraction within the bay. The total and longshore component of wave energy flux are determined for each wave class. The total annual energy flux P1 and the net longshore component P1 are determined for 55 points spaced at one kilometre intervals by multiplying the value of energy flux at each point by the mean annual frequency for that wave class and summing the values for all relevant waves. A simple model of potential erosion, transport, and deposition patterns within the bay is then developed from the variation in the net P1 values. The predicted model is compared with geomorphological evidence of sediment transport patterns visible in vertical aerial photographs and with measurements of erosion and accretion at selected points. It is concluded that the continued eastward growth of the Wasaga spit results primarily from deposition of sediments from the Nottawasaga River rather than littoral transport.},
	language = {en},
	number = {1},
	urldate = {2023-03-06},
	journal = {Journal of Great Lakes Research},
	author = {Davidson-Arnott, Robin G. D. and Pollard, Wayne H.},
	month = jan,
