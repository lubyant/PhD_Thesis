	note = {Publisher: The Royal Society},
	pages = {241--258},
	file = {JSTOR Full Text PDF:files/196/Longuet-Higgins - 1983 - On the Joint Distribution of Wave Periods and Ampl.pdf:application/pdf},
}

@article{huang_joint_2020,
	title = {Joint distribution of individual wave heights and periods in mixed sea states using finite mixture models},
