}

@article{zeng_characteristics_2025,
	title = {Characteristics of the water extent and width of endorheic {Tibetan} {Plateau} rivers revealed by {Sentinel}-2},
	volume = {658},
	issn = {0022-1694},
	url = {https://www.sciencedirect.com/science/article/pii/S0022169425005293},
	doi = {10.1016/j.jhydrol.2025.133191},
	abstract = {The endorheic Tibetan Plateau (TP) is a hotspot affected by climate change, yet the hydrological characteristics of rivers and the factors driving their development have not been clearly elucidated. To address this, we proposed a remote sensing framework to reveal river water extent and width across multiple spatial and temporal scales. A multi-temporal water frequency layer was generated using all Sentinel-2 MSI images observed in unfreezing periods between 2019 and 2021. Three river water inundation states, including the maximum state (10Â \% water frequency), the median state (50Â \% water frequency), and the instantaneous state (single-date observation in the wet-to-dry-transition season), were described separately. Water frequency maps were compared with global surface water datasets, and river width estimates were validated against field measurements. The results showed high consistency between the derived data and the reference dataset. The river width showed strong evaluation metrics, with a R2 of 0.83, a root mean square error of 15.26Â m, a mean absolute error of 12.71Â m, and a mean absolute percentage error of 17.32Â \%. At maximum inundation, endorheic TP rivers (max. widthÂ {\textgreater}Â 10Â m) spanned 20,686.67Â km, covering 1,514.92Â km2, with an average width of 87.53Â m and a maximum of 3,763.82Â m, yielding a drainage density of 0.02Â km/km2. In the median (instantaneous) states, the total river area, reach length, and average width accounted for 34.27Â \% (48.26Â \%), 60.16Â \% (79.49Â \%), and 32.29Â \% (41.51Â \%) of the maximum state. The development of these endorheic TP rivers was significantly influenced by tectonic landforms and glacier distribution, while climate conditions primarily shaped drainage density, showing a nonlinear trend with increasing aridity.},
	urldate = {2025-04-04},
	journal = {Journal of Hydrology},
	author = {Zeng, Fanxuan and Liu, Kai and Zhao, Yongquan and Xu, Pengju and Deng, Xinyuan and Chen, Tan and Song, Chunqiao},
	month = sep,
	year = {2025},
	keywords = {Remote sensing, Endorheic, River width, Sentinel-2 constellation, Spatial pattern, Tibetan Plateau},
	pages = {133191},
	file = {ScienceDirect Full Text PDF:files/1407/Zeng et al. - 2025 - Characteristics of the water extent and width of endorheic Tibetan Plateau rivers revealed by Sentin.pdf:application/pdf;ScienceDirect Snapshot:files/1408/S0022169425005293.html:text/html},
}

