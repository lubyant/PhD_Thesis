Publisher: Elsevier
WOS:000899954200014},
	keywords = {trends, variability, Block   maxima method, Extreme value distributions, Extreme water levels, setup, Wind direction},
	pages = {102741},
	file = {Full Text:files/366/Mannikus and Soomere - 2023 - Directional variation of return periods of water l.pdf:application/pdf},
