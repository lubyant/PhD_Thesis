WOS:000222777400001},
	keywords = {Lake Michigan, prediction model, sediment resuspension by waves, transport, wave model},
	pages = {C10S03},
	file = {Hawley et al. - 2004 - A comparison of observed and modeled surface waves.pdf:files/253/Hawley et al. - 2004 - A comparison of observed and modeled surface waves.pdf:application/pdf},
}

@article{zounemat-kermani_time_2015,
