	file = {Mattheus et al. - 2022 - Great lakes urban pocket-beach dynamics A GIS-bas.pdf:files/324/Mattheus et al. - 2022 - Great lakes urban pocket-beach dynamics A GIS-bas.pdf:application/pdf},
}

@article{mikulic_wave_2023,
