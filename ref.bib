@article{ranasinghe_southern_2004-1,
	title = {The {Southern} {Oscillation} {Index}, wave climate, and beach rotation},
	volume = {204},
	issn = {0025-3227},
	url = {https://www.sciencedirect.com/science/article/pii/S0025322704000027},
	doi = {10.1016/S0025-3227(04)00002-7},
	abstract = {Short embayed beaches bounded by headlands are a common feature along the southern and central coastline of New South Wales, Australia. Many of these embayed beaches have experienced severe erosion at their southern end over the last decade. Previous studies have suggested that this erosion may be the result of an oscillatory medium-term phenomenon known as beach rotation. The present study was undertaken with the objectives of: (1) establishing definitive links between the Southern Oscillation Index (SOI), wave climate, and beach rotation, and (2) determining the physical processes governing beach rotation. Data from two similar New South Wales beaches were analysed using time series analysis techniques and image processing techniques (using ARGUS video images) in this study. The results indicate that the northern end of this type of beach accretes during El NiÃ±o phases, while the southern end erodes, resulting in a net clockwise rotation of the beach. The opposite occurs during La NiÃ±a phases resulting in a net anti-clockwise rotation of the beach. The beach response at the northern and southern ends lags SOI trend shifts by 3 and 17 months, respectively. Waves are predominantly incident from the southeast during both El NiÃ±o and La NiÃ±a phases. Offshore wave height is positively correlated with the SOI while offshore wave direction is negatively correlated with the SOI. On average, the number of storms per year doubles from El NiÃ±o to La NiÃ±a. Based on these links between beach width fluctuations, SOI, and incident wave conditions, a conceptual model of beach rotation is presented. The model describes the combinations of cross-shore and longshore sediment transport and hydrodynamic processes that are expected to result in the observed clockwise and anti-clockwise beach rotation during El NiÃ±o and La NiÃ±a phases, respectively.},
	number = {3},
	urldate = {2023-08-20},
	journal = {Marine Geology},
	author = {Ranasinghe, Roshanka and McLoughlin, Rodney and Short, Andrew and Symonds, Graham},
	month = mar,
	year = {2004},
	keywords = {beach rotation, Narrabeen Beach, Palm Beach, Southern Oscillation Index, video imaging},
	pages = {273--287},
	file = {ScienceDirect Full Text PDF:files/535/Ranasinghe et al. - 2004 - The Southern Oscillation Index, wave climate, and .pdf:application/pdf;ScienceDirect Snapshot:files/536/S0025322704000027.html:text/html},
}

@article{bergillos_coupling_2017,
	title = {Coupling cross-shore and longshore sediment transport to model storm response along a mixed sand-gravel coast under varying wave directions},
	volume = {129},
	issn = {03783839},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0378383916303246},
	doi = {10.1016/j.coastaleng.2017.09.009},
	abstract = {This paper investigates the proï¬le response of a mixed sand-gravel deltaic beach (Playa Granada, southern Spain) forced by storm waves from varying directions. Beach morphology was monitored over a 36-day period with variable wave conditions, and proï¬le response was compared to model predictions using the XBeach-G model and a longshore sediment transport (LST) formulation. XBeach-G was applied over 2-day periods of low energy, southwesterly (SW) storm and south-easterly (SE) storm conditions, and was coupled to LST using a parametric approach which distributes the LST across the swash, surf and nearshore zones. A calibrated wave propagation model (Delft3D) was used to obtain the inshore conditions required to drive the XBeach-G model and the LST formulation. The storm response is clearly inï¬uenced by the free-board (difference between the height of the berm and the total run-up) and is also strongly dependent on storm-wave direction, with the SW storm eroding the surveyed area, while the SE storm induced beach accretion. Model results indicate that XBeach-G on its own is capable of adequately reproducing the response of the beach under SW storm conditions (BSS {\textgreater} 0:95), but not under SE storms due to the higher LST gradients at the study location. The combination of XBeach-G and LST ï¬ts the measured proï¬les reasonably well under both SW (BSS {\textgreater} 0:96) and SE (BSS {\textgreater} 0:88) storms, inspiring conï¬dence in the coupled model to predict the storm response under varying wave conditions. The combined XBeachG/LST model was applied to the entire 6.8-km deltaic coastline to investigate the impact of extreme SW and SE storm events, and the model results reiterate the importance of cross-shore and longshore sediment transport in driving coastal storm response at this location. The approach proposed in this work can be extended to other worldwide coasts highly inï¬uenced by both cross-shore and longshore sediment transport, such as beaches with different coastline orientations and/or forced by varying wave directions.},
	language = {en},
	urldate = {2023-08-20},
	journal = {Coastal Engineering},
	author = {Bergillos, Rafael J. and Masselink, Gerd and Ortega-SÃ¡nchez, Miguel},
	month = nov,
	year = {2017},
	pages = {93--104},
	file = {Bergillos et al. - 2017 - Coupling cross-shore and longshore sediment transp.pdf:files/537/Bergillos et al. - 2017 - Coupling cross-shore and longshore sediment transp.pdf:application/pdf},
}

@article{wiggins_coastal_2019,
	title = {Coastal embayment rotation: {Response} to extreme events and climate control, using full embayment surveys},
	volume = {327},
	issn = {0169-555X},
	shorttitle = {Coastal embayment rotation},
	url = {https://www.sciencedirect.com/science/article/pii/S0169555X1830463X},
	doi = {10.1016/j.geomorph.2018.11.014},
	abstract = {Barrier beach change in directionally bi-modal wave climates presents an increasing challenge for coastal communities, both in the short-term (storm events), and decadal to centurial time scales (long-term evolution). Predicting and planning for subsequent variations requires understanding of the morphological response to changes in wave energy, along with the atmospheric forces driving the wave climate. In this paper, multi-method topo-bathymetric surveys are used to assess the morphological change of a semi-sheltered gravel barrier (Start Bay, Devon, UK). Total sediment budgets (supra- to sub-tidal), with spatially-varying uncertainty levels, indicate the embayment is closed. One third of total sediment flux occurred in the sub-tidal, establishing the importance of sub-tidal transport for this type of coastline. Our results demonstrate that under the predominance of a given wave direction, rotation first occurs within sub-embayments. Additional sustained and extreme energy levels are then required for full embayment rotation to occur, with significant headland bypassing. In this instance, 6â¯Ãâ¯105â¯m3 of gravel was transported alongshore during a 3-year sustained period of dominant-southerly waves, including a 1:50â¯year storm season (full-embayment rotation), whilst 3â¯Ãâ¯105â¯m3 was returned during a 2-year period of dominant easterly waves (sub-embayment rotation only). A novel parameter is introduced that predicts beach rotation based on the directional wave balance. In turn, winter wave direction is shown to correlate with a combination of two climate indices. Given adequate predictions of relevant climate indices, these findings constitute the basis of a generalisable method to predict and plan for future beach rotation on similar beaches globally.},
	urldate = {2023-08-20},
	journal = {Geomorphology},
	author = {Wiggins, Mark and Scott, Tim and Masselink, Gerd and Russell, Paul and McCarroll, Robert Jak},
	month = feb,
	year = {2019},
	keywords = {Beach morphodynamics, Beach rotation, Headland bypassing, Sediment budget, Storm response},
	pages = {385--403},
	file = {ScienceDirect Full Text PDF:files/540/Wiggins et al. - 2019 - Coastal embayment rotation Response to extreme ev.pdf:application/pdf;ScienceDirect Snapshot:files/541/S0169555X1830463X.html:text/html},
}

