Publisher: Elsevier Sci Ltd
WOS:000606413700004},
	keywords = {Machine learning, algorithm, prediction, Breaking wave detection, breaking waves, driven, Earthquake detection, full-scale, neural-network, seismic phase, Shapelet transform, suspension bridge, thunderstorm, Thunderstorm classification, Time series classification, Time series shapelets, vortex-induced vibration},
	pages = {111564},
	file = {Submitted Version:files/271/Arul and Kareem - 2021 - Applications of shapelet transform to time series .pdf:application/pdf},
}

