Publisher: Elsevier
WOS:000315557900007},
	keywords = {projections, trends, wind-waves, hindcast, reanalysis, north-atlantic, Seasonality, ocean wind, Inter-annual variability, Long-term trends, Ocean waves, pacific, sea-surface temperatures, storm tracks, Wave direction},
	pages = {70--84},
