@article{kayastha_reconstructing_2023,
	title = {Reconstructing 42 {Years} (1979â2020) of {Great} {Lakes} {Surface} {Temperature} through a {Deep} {Learning} {Approach}},
	volume = {15},
	issn = {2072-4292},
	url = {https://www.mdpi.com/2072-4292/15/17/4253},
	doi = {10.3390/rs15174253},
	abstract = {Accurate estimates for the lake surface temperature (LST) of the Great Lakes are critical to understanding the regional climate. Dedicated lake models of various complexity have been used to simulate LST but they suffer from noticeable biases and can be computationally expensive. Additionally, the available historical LST datasets are limited by either short temporal coverage ({\textless}30 years) or lower spatial resolution (0.25â¦ Ã 0.25â¦). Therefore, in this study, we employed a deep learning model based on Long Short-Term Memory (LSTM) neural networks to produce a daily LST dataset for the Great Lakes that spans an unparalleled 42 years (1979â2020) at a spatial resolution of {\textasciitilde}1 km. In our dataset, the Great Lakes are represented by {\textasciitilde}33,000 unstructured grid points and the LSTM training incorporated the information from each grid point. The LSTM was trained with seven meteorological variables from reanalysis data as feature variables and the LST from a historical satellite-derived dataset as the target variable. The LSTM was able to capture the spatial heterogeneity of LST in the Great Lakes well and exhibited high correlation (â¥0.92) and low bias (limited to Â±1.5 â¦C) for the temporal evolution of LST during the training (1995â2020) and testing (1979â1994) periods.},
	language = {en},
	number = {17},
	urldate = {2024-03-15},
	journal = {Remote Sensing},
	author = {Kayastha, Miraj B. and Liu, Tao and Titze, Daniel and Havens, Timothy C. and Huang, Chenfu and Xue, Pengfei},
	month = aug,
	year = {2023},
	pages = {4253},
	file = {Kayastha et al. - 2023 - Reconstructing 42 Years (1979â2020) of Great Lakes.pdf:files/780/Kayastha et al. - 2023 - Reconstructing 42 Years (1979â2020) of Great Lakes.pdf:application/pdf},
}

@misc{abdelhady_machine_2023,
