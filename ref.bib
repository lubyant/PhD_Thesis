Publisher: Mdpi
WOS:000915475500001},
	keywords = {Adriatic Sea, annual maxima, directional maxima, long-term probability, monthly maxima, seasonal variability effects, significant wave height, wave   directionality effects, wind direction},
	pages = {42},
	file = {Full Text:files/327/Mikulic and Parunov - 2023 - A Wave Directionality and a Within-Year Wave Clima.pdf:application/pdf},
}

