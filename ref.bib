@article{gronewold_tug--war_2021,
	title = {A {Tug}-of-{War} {Within} the {Hydrologic} {Cycle} of a {Continental} {Freshwater} {Basin}},
	volume = {48},
	copyright = {Â© 2021. American Geophysical Union. All Rights Reserved. This article has been contributed to by US Government employees and their work is in the public domain in the USA.},
	issn = {1944-8007},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1029/2020GL090374},
	doi = {10.1029/2020GL090374},
	abstract = {The past decade was the wettest on record for much of central and eastern North America. Near the beginning of this period of regional water abundance, however, drought conditions reinforced concerns that high temperatures and evapotranspiration foreshadowed a persistent imbalance in the hydrologic cycle characterized by water loss. These fluctuating hydrologic conditions were manifest by water level variability on the Laurentian Great Lakes, the largest system of lakes on Earth. We show that, during this period, the two dominant hydrologic forces acting directly on the vast surfaces of the lakes, overlake precipitation and overlake evaporation, have evolved differently. More specifically, we find that overlake precipitation has risen to extraordinary levels, while overlake evaporation diminished rapidly in 2014 (coinciding with a strong Arctic polar vortex deformation). Our findings offer a new perspective on the impacts of competing hydrologic forces on large freshwater systems in an era of climate change.},
	language = {en},
	number = {4},
	urldate = {2025-02-03},
	journal = {Geophysical Research Letters},
	author = {Gronewold, A. D. and Do, H. X. and Mei, Y. and Stow, C. A.},
	year = {2021},
	note = {\_eprint: https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2020GL090374},
	keywords = {water levels, water balance, precipitation, evaporation, large lakes},
	pages = {e2020GL090374},
	annote = {e2020GL090374 2020GL090374},
	file = {Full Text PDF:files/1261/Gronewold et al. - 2021 - A Tug-of-War Within the Hydrologic Cycle of a Continental Freshwater Basin.pdf:application/pdf;Snapshot:files/1262/2020GL090374.html:text/html},
