	author = {Lee, Hong Sik and Kim, Sung Duk and Wang, K. -H. and Eom, Sieun},
	month = dec,
	year = {2009},
	keywords = {Boundary element method, Dredging, Harbor, Navigation channel, Random waves, Surface wave-structureâbottom interaction},
	pages = {1287--1294},
	file = {ScienceDirect Full Text PDF:files/1093/Lee et al. - 2009 - Boundary element modeling of multidirectional random waves in a harbor with a rectangular navigation.pdf:application/pdf;ScienceDirect Snapshot:files/1094/S002980180900239X.html:text/html},
}

@incollection{mackay_803_2012,
	address = {Oxford},
	title = {8.03 - {Resource} {Assessment} for {Wave} {Energy}},
