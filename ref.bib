	keywords = {Wave spectra, Buoys, Wave data},
	pages = {1421--1434},
	file = {ScienceDirect Full Text PDF:files/1220/Earle et al. - 1999 - Use of advanced directional wave spectra analysis methods.pdf:application/pdf;ScienceDirect Snapshot:files/1221/S0029801899000104.html:text/html},
}

