	urldate = {2024-07-12},
	journal = {Sci Data},
	author = {Hall, Candice and Jensen, Robert E.},
	month = may,
	year = {2022},
	note = {Publisher: Nature Publishing Group},
	keywords = {Physical oceanography, Environmental monitoring, Natural hazards, Water resources},
	pages = {248},
	file = {Full Text PDF:files/861/Hall and Jensen - 2022 - USACE Coastal and Hydraulics Laboratory Quality Co.pdf:application/pdf},
}

