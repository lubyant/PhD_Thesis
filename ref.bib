	number = {C10},
	urldate = {2022-12-25},
	journal = {J. Geophys. Res.-Oceans},
	author = {Hawley, N. and Lesht, B. M. and Schwab, D. J.},
	month = jun,
	year = {2004},
	note = {Place: Washington
