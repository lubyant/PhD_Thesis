	file = {arXiv.org Snapshot:files/758/2403.html:text/html;Full Text PDF:files/759/Zhao et al. - 2024 - GaLore Memory-Efficient LLM Training by Gradient .pdf:application/pdf},
}

@misc{ouyang_training_2022,
